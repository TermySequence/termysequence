if ([ "$1" = "--help" ] || [ $# -lt 1 ]); then
    echo "Usage: @ABBREV_NAME@-ssh [ssh options] [user@]host" 1>&2
    exit 1
fi
if [ "$1" = "--version" ]; then
    echo "@ABBREV_NAME@-ssh @PROJECT_VERSION@"
    exit
fi

exec @CONNECT_NAME@ -p -- ssh -qT "$@" @SERVER_NAME@
